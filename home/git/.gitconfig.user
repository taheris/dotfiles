[user]
  name=Shaun Taheri
  email=gpg@taheris.net
  signingkey = 1C2BE6B85F923891
[commit]
  gpgsign = true
[core]
  autocrlf = false
  editor = vim
[merge]
  tool = vim
[mergetool "vim"]
  cmd="vim -c 'diff' $MERGED"
[push]
  showUntrackedFiles = all
[rebase]
  autostash=true
[stash]
  showPatch=true
[diff]
  tool = difftastic
[difftool]
  prompt = false
[difftool "difftastic"]
  cmd = difft "$LOCAL" "$REMOTE"
[pager]
  difftool = true
[url "git@github.com:"]
  insteadOf = https://github.com/
